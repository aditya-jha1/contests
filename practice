#include <iostream>
using namespace std;
int main(void){
    // Your code here!
    int t;
    cin>>t;
    while(t--)
    {
        int a,b;cin>>a>>b;
        int i,s1=0,s2=0;
        for(i=1;;i++)
        {
            if(i%2!=0)
            s1=s1+i;
            if(s1>a)
            {cout<<"Bob"<<endl;break;}
            if(i%2==0)
            s2+=i;
            if(s2>b)
            {cout<<"Limak"<<endl;break;}
            
        }
        
    }
}
